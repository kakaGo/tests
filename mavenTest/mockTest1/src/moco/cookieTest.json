  {
    "description": "这是一个带cookie的post请求",
    "request": {
      "uri": "/postWithcookie",
      "method": "post",
      "cookies": {
        "login": "true"
      }
    },
    "response":{
      "text":"这是一个需要携带cookies信息才能访问的post请求！！",
      "headers": {
        "Context-Type": "text/html;charset=gbk"
      }
